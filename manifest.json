{
  "name": "Devtools Reloader",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Reload pages in debugging",
  "homepage_url": "http://tiye.me",
  "web_accessible_resources": [
    "src/*"
  ],
  "background": {
    "scripts": [
      "src/reload.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}